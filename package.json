{
  "name": "ng-upgrade-orchestrator",
  "version": "1.1.0",
  "description": "Enterprise-grade Angular Multi-Version Upgrade Orchestrator with automatic npm installation, comprehensive dependency management, and seamless integration of all 9 official Angular migrations. Safely migrate Angular applications across multiple major versions (12â†’20) with zero functionality loss.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ng-upgrade": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "examples/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 80",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test && npm run docs:generate",
    "prepare": "npm run build",
    "ng-upgrade": "node dist/cli.js",
    "demo:analyze": "node dist/cli.js analyze",
    "demo:upgrade": "node dist/cli.js upgrade --target 20 --dry-run",
    "demo:basic": "node examples/basic-upgrade.js",
    "docs:generate": "compodoc src -p tsconfig.json -d docs/api --theme material --name \"Angular Multi-Version Upgrade Orchestrator\" --hideGenerator --disableCoverage false --disablePrivate false --disableProtected false --disableInternal false --disableLifeCycleHooks false --disableRoutesGraph true",
    "docs:serve": "compodoc -s -p tsconfig.json -d docs --theme material --name \"Angular Multi-Version Upgrade Orchestrator\"",
    "docs:watch": "compodoc -p tsconfig.json -d docs --theme material --name \"Angular Multi-Version Upgrade Orchestrator\" --watch",
    "docs:coverage": "compodoc -p tsconfig.json --coverageTest 90 --coverageTestThresholdFail true",
    "docs:stats": "compodoc -p tsconfig.json --exportFormat json -d docs/stats",
    "docs:clean": "rimraf docs/index.html docs/js docs/css docs/images docs/fonts"
  },
  "keywords": [
    "angular",
    "upgrade",
    "migration",
    "orchestrator",
    "multi-version",
    "npm-install",
    "dependency-management",
    "official-migrations",
    "standalone-components",
    "control-flow",
    "signals",
    "angular12",
    "angular13",
    "angular14",
    "angular15",
    "angular16",
    "angular17",
    "angular18",
    "angular19",
    "angular20",
    "cli",
    "automation",
    "enterprise",
    "zero-downtime",
    "ast-transformations"
  ],
  "author": {
    "name": "sahassakhare",
    "url": "https://github.com/sahassakhare"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahassakhare/ng-upgrade.git"
  },
  "bugs": {
    "url": "https://github.com/sahassakhare/ng-upgrade/issues"
  },
  "homepage": "https://github.com/sahassakhare/ng-upgrade#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular-devkit/schematics": "^20.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "semver": "^7.6.0",
    "ts-morph": "^21.0.1",
    "typescript": "^5.6.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
